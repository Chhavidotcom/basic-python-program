a=int(input('enter first number a '))
b=int(input('enter second number b '))
print("Prime numbers between", a, "and", b, "are:")

for num in range(a, b + 1):
   # all prime numbers are greater than 1
   if num > 1:
       for i in range(2, num):
           if (num % i) == 0:
               break
       else:
           print(num)
